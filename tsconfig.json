{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "types": ["cypress", "node"],
    "typeRoots": ["node_modules/@types"],
    "moduleResolution": "node",
    "lib": ["es5", "dom"],
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "**/node_modules/*"],
  "compileOnSave": true
}